<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="jcGcCache" updateCheck="false">

<!--     <diskStore path="java.io.tmpdir"/> -->

<!--         maxElementsInMemory="1" -->
	<!-- 登录记录缓存 锁定10分钟 -->
    <cache name="jc_cache"
        maxEntriesLocalHeap="300"
        eternal="true"
        timeToIdleSeconds="3600"
        timeToLiveSeconds="0"
        overflowToDisk="false"
        statistics="true">
    </cache>
    
    <cache name="jc_gc_cache"
        maxEntriesLocalHeap="300"
        eternal="true"
        timeToIdleSeconds="3600"
        timeToLiveSeconds="0"
        overflowToDisk="false"
        statistics="true">
    </cache>
	
</ehcache>
